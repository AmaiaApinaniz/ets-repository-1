<TranslationTemplateBundle xmlns="http://www.interactive-instruments.de/etf/2.0"
	id="EID6f004d02-ca77-4a67-99a4-62515725a573"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.interactive-instruments.de/etf/2.0 https://resources.etf-validator.net/schema/v2/service/service.xsd">
	<translationTemplateCollections>
		<LangTranslationTemplateCollection name="TR.multiplicityShallBeOne">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.multiplicityShallBeOne">Exactly one '{element}' element shall be provided. It was found 0 or more than 1.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.multiplicityShallBeZeroOrOne">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.multiplicityShallBeZeroOrOne">Zero or one '{element}' element shall be provided. It was found more than 1.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.multiplicityShallBeOneOrMore">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.multiplicityShallBeOneOrMore">One or more '{element}' elements shall be provided. It was found 0.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.valueShallNotBeEmpty">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.valueShallNotBeEmpty">The value of '{element}' shall not be empty.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.valueShallBeUnique">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.valueShallBeUnique">The value of '{element}' shall be unique.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.extendedCapabilitiesShallBeProvided">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.extendedCapabilitiesShallBeProvided">The ExtendedCapabilities section shall be provided in the path: /Capabilities/OperationsMetadata/ExtendedCapabilities</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.invalidResponseToGetCapabilitiesLanguaged">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.invalidResponseToGetCapabilitiesLanguaged">The response to GetCapabilities with 'LANGUAGE' parameter is invalid.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.requestedAndResponseLanguageDontMatch">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.requestedAndResponseLanguageDontMatch">The response '{response}' language to GetCapabilities request is not the same as requested '{requested}'.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.invalidLanguageCode">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.invalidLanguageCode">The language '{element}' is not a valid ISO 639-2/B alpha 3 code.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.invalidResponseToGetCapabilitiesLanguagedWithoutParameter">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.invalidResponseToGetCapabilitiesLanguagedWithoutParameter">The response to GetCapabilities without 'LANGUAGE' parameter is not correct. Default language was expected as ResponseLanguage.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.invalidResponseToGetCapabilitiesLanguagedWithWrongParameter">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.invalidResponseToGetCapabilitiesLanguagedWithWrongParameter">The response to GetCapabilities with a wrong 'LANGUAGE' parameter is not correct. Default language was expected as ResponseLanguage.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.invalidSchema">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.invalidSchema">Schema not valid: {error}</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.metadataURLNotWellFormed">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.metadataURLNotWellFormed">MetadataURL/URL element in ExtendedCapabilities is not a well formed url.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.missingSupportedQueryableValue">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.missingSupportedQueryableValue">In the path /Capabilities/OperationsMetadata/Operation[@name="GetRecords"]/Constraint[@name="SupportedISOQueryables"]/Value the following mandatory element(s) is(are) missing: {value}.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.missingAdditionalQueryableValue">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.missingAdditionalQueryableValue">In the path /Capabilities/OperationsMetadata/Operation[@name="GetRecords"]/Constraint[@name="AdditionalQueryables"]/Value the following mandatory element(s) is(are) missing: {value}.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.invalidGetRecordsResponse">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.invalidGetRecordsResponse">The response to a GetRecords request is not valid.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.languagesNotISO">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.languagesNotISO">The following language(s) is(are) not ISO 639-2/B alpha 3 valid: {language}</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
		<LangTranslationTemplateCollection name="TR.reponseTitleAbstractShallBeDefault">
			<translationTemplates>
				<TranslationTemplate language="en" name="TR.reponseTitleAbstractShallBeDefault">The response Title and Abstract shall be the same as default ones when default or invalid language is requested.</TranslationTemplate>
			</translationTemplates>
		</LangTranslationTemplateCollection>
	</translationTemplateCollections>
</TranslationTemplateBundle>
