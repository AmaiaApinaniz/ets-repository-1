			<testCases>
				<TestCase id="EID5f7e3e70-dbd5-449c-82d7-5374354b6bd7">
					<label>General requirements</label>
					<description>Execute tests that apply for all metadata records on elements that are directly child elements of the root node of the XML document: MD_Metadata node.
					</description>
					<parent ref="EID5f19352f-40f8-4a21-8cd0-4117e85cc511"/>
					<testSteps>
						<TestStep id="EID17947c8a-250d-4eea-88d9-d4d39dc6c907">
							<label>IGNORE</label>
							<description>IGNORE</description>
							<parent ref="EID5f7e3e70-dbd5-449c-82d7-5374354b6bd7"/>
							<statementForExecution>not applicable</statementForExecution>
							<testItemType ref="EIDf483e8e8-06b9-4900-ab36-adad0d7f22f0"/>
							<testAssertions>

								<TestAssertion id="EIDb4849d77-d7ca-465c-94dc-d6d71ffd6cd1">
									<label>M.1: Spatial scope code list</label>
									<description><![CDATA[<p>Test that the spatial scope covered by the data set, data series or service is provided.</p>
									<p>More information: <a href="http://inspire.ec.europa.eu/id/ats/metadata/2.0/monitoring/spatial-scope" target="_blank">Spatial Scope Code List</a></p>]]></description>
									<parent ref="EID17947c8a-250d-4eea-88d9-d4d39dc6c907"/>
									<expectedResult>NOT_APPLICABLE</expectedResult>
									<expression>
let $messages := 
	(for $record in $records
	let $rid := $record/gmd:fileIdentifier/*/text()
	return
	()
	)[position() le $limitErrors]
return
(if ($messages) then 'FAILED' else 'PASSED',
 local:error-statistics('TR.recordsWithErrors', count(fn:distinct-values($messages//etf:argument[@token='id']/text()))),
 $messages)
							  		</expression>
									<testItemType ref="EIDf0edc596-49d2-48d6-a1a1-1ac581dcde0a"/>
									<translationTemplates>
										<translationTemplate ref="TR.spatialScopeCodeListMissing"/>
										<translationTemplate ref="TR.recordsWithErrors"/>
									</translationTemplates>
								</TestAssertion>

							</testAssertions>
							</TestStep>
					</testSteps>
